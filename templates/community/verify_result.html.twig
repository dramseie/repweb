{% extends 'base.html.twig' %}
{% block title %}Membership verification{% endblock %}
{% block body %}
<div class="container py-5" style="max-width: 560px;">
  <h1 class="mb-3">Membership verification</h1>
  {% if ok %}
    <div class="alert alert-success">{{ message }}</div>
  {% else %}
    <div class="alert alert-danger">{{ message }}</div>
  {% endif %}
  <a class="btn btn-outline-secondary" href="{{ path('community_apply') }}">Back</a>
</div>
{% endblock %}
